

struct registration_command_t

# timestamp of this message
int64 utime

# command to apply
int8 command

# command types
int8 RESET = 1             # reset the registration module
int8 AFF_UPDATE_START = 2  # start updating affordances
int8 AFF_UPDATE_PAUSE = 3  # stop updating affordances
int8 AFF_UPDATE_FORCE = 4  # force one-time affordance update

